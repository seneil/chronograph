    "typescript": "4.4.4",
    "webpack": "5.60.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -q"
