{
  "name": "chronograph-app",
  "productName": "chronograph-app",
  "version": "0.0.1",
  "description": "Chronograph Electron Application",
  "engines": {
    "node": "14.18.0",
    "npm": ">=6.14.15-fb <7.0.0"
  },
  "private": true,
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "start:inspect": "electron-forge start --inspect-electron",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "migrate:seed": "knex seed:run --knexfile ./application/knexfile.ts",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest --collect-coverage"
  },
  "keywords": [],
  "author": {
    "name": "Dmitry Shvalyov",
    "email": "dmitry@shvalyov.ru"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0",
    "@electron-forge/maker-deb": "^6.0.0",
    "@electron-forge/maker-rpm": "^6.0.0",
    "@electron-forge/maker-squirrel": "^6.0.0",
    "@electron-forge/maker-zip": "^6.0.0",
    "@electron-forge/plugin-webpack": "^6.0.0",
    "@electron/rebuild": "^3.2.10",
    "@funboxteam/scss-vars-loader": "^2.2.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.9",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "css-loader": "^6.7.1",
    "electron": "22.0.0",
    "eslint": "^8.28.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^29.3.1",
    "node-loader": "^2.0.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "better-sqlite3": "^8.0.0",
    "dayjs": "^1.11.7",
    "electron-squirrel-startup": "^1.0.0",
    "knex": "^2.4.0"
  }
}
